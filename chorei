Algoritmo "bah"

var
 i, f1, f2, p, c : inteiro
 vf: logico
inicio
 escreva("Qual valor deseja comparar com Fibonacci: ")
 leia(c)

 f1 <- 0
 f2 <- 1
 vf <- falso

 escreval(f1)
 escreval(f2)

 para i de 3 ate 13 faca
  p <- f1 + f2
  escreval(p)

  f1 <- f2
  f2 <- p
  
  se c = p entao
    vf <- verdadeiro
  fimse
 fimpara
 
  se vf = verdadeiro entao
   escreva("O valor pertence a Fabonacci")
    senao
    escreva("O valor nao pertence a Fabonacci")
  fimse
fimalgoritmo
