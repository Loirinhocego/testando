Algoritmo "bah"

Var
 t: caractere
 l: caractere
 i, cont: inteiro

Inicio
 cont <- 0

 escreva("Digite o texto: ")
  leia(t)
  escreva("Digite a letra que deseja contar: ")
   leia(l)
   
 para i de 1 ate compr(t) faca
  se t[i] = l ou t[i] = maiusc(l) ou t[i] = minusc(l) entao
   cont <- cont + 1
   fimse
   fimpara

 escreval("A letra '", l, "' aparece ", cont, " vezes no texto.")
